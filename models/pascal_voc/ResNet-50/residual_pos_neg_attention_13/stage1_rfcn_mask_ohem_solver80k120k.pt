train_net: "models/pascal_voc/ResNet-50/residual_pos_neg_attention_13/stage1_rfcn_ohem_train.pt"
# resmask on cls and bbox, with ohem, lr 0.1, 21 map without deconv, without mask reg.
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 60000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "resnet50_rfcn_mask_ohem"
iter_size: 2
