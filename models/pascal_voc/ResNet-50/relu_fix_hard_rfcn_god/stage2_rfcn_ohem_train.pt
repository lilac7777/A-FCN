    top:"softroipooled_cls_reshape"
    reshape_param{shape { dim:  0  dim:  21 dim: 7 dim: 7}} #21, K
}

layer {
  name: "softroipooled_cls"
  type: "Pooling"
  bottom: "softroipooled_cls_reshape"
  top: "cls_score_soft"
  pooling_param {
